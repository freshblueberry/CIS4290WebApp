            -moz-transition: all 0.5s;
            -o-transition: all 0.5s;
            transition: all 0.5s;
            i{
              font-size: 45px;
              color: $brand-secondary;
              display: inline-block;
              vertical-align: middle; 
              width: 26px;
              height: 26px;
              line-height: 26px;
              border-radius: 30px;
              -webkit-transition: all 0.5s;
              -moz-transition: all 0.5s;
              -o-transition: all 0.5s;
              transition: all 0.5s;
              &:hover{                
