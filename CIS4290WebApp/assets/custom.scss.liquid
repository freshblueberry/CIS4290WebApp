/*================ Services block ================*/
.ishi-service-block{
  #ishiservices{
  text-align: center;
  position: relative;
  .ishiservices-content{
    display: block;
    overflow: hidden;
    position: relative;
    text-align: center;
    margin: 0 auto;
  }
  .service-block{
    display: block;
    overflow: hidden;
      border: 1px solid #e8e8e8;
  }
  .services{
      text-align: center;
      cursor: pointer;
      padding: 40px 15px;
      -webkit-transition: all 0.5s;
      -moz-transition: all 0.5s;
      -o-transition: all 0.5s;
      transition: all 0.5s;
      @media (min-width: 544px) {/* If media is above 543 */
        border-right: 1px solid #e8e8e8;
        &:last-child{
          border-right: 0;
        }
        @media (max-width: 991px) {/* If media is below 992 */
          border-bottom: 1px solid #e8e8e8;
          &:nth-child(2){
            border-right: 0;
          }
          &:nth-child(3), &:last-child{
            margin-bottom: 0;
            border-bottom: 0;
          }
        }
        @media (max-width: 767px) {/* If media is below 768 */
          padding: 25px 10px;
        }
      }
      @media (max-width: 543px) {/* If media is below 544 */
        width: 100%;
        padding: 15px;
        border-bottom: 1px solid #e8e8e8;
        &:last-child{
          border-bottom: 0;
        }
      }
      .service-img-outer{
        display: inline-block;
        vertical-align: middle;
        position: relative;
        background: transparent;
        @media screen and (min-width: 992px) and (max-width: 1199px) {/* If media is between 992 and 1199 */
          text-align: center;
          margin: 0 auto 5px;
        }
        @media (max-width: 767px) {/* If media is below 768 */
          display: block;
          text-align: center;
          margin: 0 auto 5px;
        }
      }
      .service-img{
        position: relative;
        display: inline-block;
        vertical-align: middle;
        margin: 0 auto 2px; 
        text-align: center;
        height: 50px;
        width: 50px;
        line-height: 50px;
        overflow: hidden;
        -webkit-transition: all 0.5s;
        -moz-transition: all 0.5s;
        -o-transition: all 0.5s;
        transition: all 0.5s;
        @media (max-width: 991px) {/* If media is below 992 */
          margin: 0 5px 0 0;
        }
        @media (max-width:767px) {
          margin: 0 0 5px 0;
        }
      }
      .service-text{
        display: block;
        @media (max-width: 991px) {/* If media is below 992 */
          display: inline-block;
          vertical-align: middle;
          text-align: left;
        }
        @media (max-width: 767px) {/* If media is below 768 */
          display: block;
          text-align: center;
        }
        .service-title{
          @media (max-width: 991px) {/* If media is below 992 */
            margin-top: 4px;
          } 
          font-size: 17px;
          font-weight: 500;
          margin: 10px 0; 
          color: #7a7a7a;
          -webkit-transition: all 0.5s;
          -moz-transition: all 0.5s;
          -o-transition: all 0.5s;
          transition: all 0.5s;
        }
        .service-desc{
          color: $brand-primary;
          font-weight: 300;
          font-size: 14px;
          transition: all 0.5s;
          -webkit-transition: all 0.5s;
          -moz-transition: all 0.5s;
          -o-transition: all 0.5s;
          transition: all 0.5s;
          @media screen and (min-width: 992px) and (max-width: 1199px) {/* If media is between 992 and 1199 */
            font-size: 12px;
          }
          @media (max-width: 543px) {/* If media is below 992 */
            font-size: 13px;
          }
        }
      }
      &:hover{  
        .service-img{
          transform: rotateY(360deg);
          -webkit-transition: rotateY(360deg);
          -moz-transition: rotateY(360deg);
          -o-transition: rotateY(360deg);
        }
        .service-text{
          .service-title{
            color: $brand-secondary;
          }
        }
      }
  }
}
}

