/*================ slide to top ================*/

#slidetop{
    background: $button-bg-color none repeat scroll 0 0;
    box-shadow: 0 0 10px 4px rgba($light-color, 0.2);
    color: transparent;
    border-radius: 50%;
    bottom: 10px;
    cursor: pointer;
    height: 40px;
    overflow: hidden;
    padding: 6px;
    position: fixed;
    right: 10px;
    text-align: center;
    width: 40px;
    z-index: 99;
    opacity: 1;
    display: none; 
    -webkit-transition: all 0.5s;
    -moz-transition: all 0.5s;
    -o-transition: all 0.5s;
    transition: all 0.5s;
    &::after{
      content: '\E5CE';
      display: inline-block;
      color:$button-text-color;
      font-family: material icons;
      font-size: 26px;
      left: 0;
      position: absolute;
      right: 0;
      top: 1px;
      -webkit-transition: all 0.5s;
      -moz-transition: all 0.5s;
      -o-transition: all 0.5s;
      transition: all 0.5s;
    }
    &:hover{
      background-color: $button-bg-hover-color;
      &:after {
        color: $button-text-hover-color;
      }
    } 
}


