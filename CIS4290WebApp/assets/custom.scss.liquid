.ishi-blog-style1{
  .smartblog{
    .blog-image{
      display: block;
      position: relative;
      width: 100%;
      background-size: cover;
      background-position:center;
      background-repeat: no-repeat;
      &.blog-color {
        display: block;
        position: relative;
        width: 100%;
      }
     }
    .products_block_inner{
      position: relative;
      .sds_blog_post{
        padding: 0 15px;
        &:hover{
          .blog_post{
            .news_module_image_holder{
              a{
                img{
                  -webkit-transform-style:unset;
                  transform-style:unset;
                  transform: scale(1.26);
                  -webkit-transform: scale(1.26);
                  -ms-transform: scale(1.26);
                  -o-transform: scale(1.26);
                  -moz-transform: scale(1.26);
                  opacity: 0.8;
                }
              }
              .blogicons{
                opacity: 1; 
              }
            }   
            .blog_content{
              .article__meta-buttons{
                .btn{
                  font-size: 14px;
                }
              }
            }      
          }     
        }

        .blog_post{
          position: relative;
          display: inline-block;
          vertical-align: middle;
          background-color: $light-color;
          box-shadow: 1px 2px 4px 1px rgba(0,0,0,0.1);
          margin: 10px 0px;
          &:hover{
            .news_module_image_holder{
              .article__date{
                -moz-transform: translateY(23px);
                -webkit-transform: translateY(23px);
                -o-transform: translateY(23px);
                -ms-transform: translateY(23px);
                transform: translateY(23px);

              }
              a{
                .comment{
                  opacity: 0;
                }
              }
            }
          }
          .news_module_image_holder{
            display: block;
            max-width: 100%;
            position: relative;
            margin-bottom: 0px;
            background: $light-color;
            a{
              position: relative;
              display: block;
              overflow: hidden;
            }
            img{
              transition: all 1.5s;
              -webkit-transition: all 1.5s;
              -moz-transition: all 1.5s;
              -ms-transition: all 1.5s;
              -o-transition: all 1.5s;
              max-width:100%; 
            }
            .smartbloginfo{                
              padding-bottom: 0;
              transition: all 0.5s;
              position: absolute;
              background:#fff;
              color:#7abb2d;
              bottom: -20px;
              left: 50px;
              right: 50px;
              text-align: center;
              margin: 0 auto;
              @media (max-width: 991px) {/* If media is below 992 */
                left: 50px;
                right: 50px;
              }
              @media (max-width: 767px) {/* If media is below 544 */
                left: 10px;
                right: 10px;
              }

              .blog_date{              
                font-family: $secondary-font;
                position: absolute;
                bottom: 0px;
                background:transparent;
                left: 0;
                right: 0;
                padding: 10px 0;
                color:$brand-primary;
                font-size: 24px;
                font-weight: 400;
                text-align: center;
                z-index: 2;
                transition: all 0.5s;
                @media (max-width: 991px) {/* If media is below 992 */
                  bottom:5px;
                }
                i{
                  display: none;
                  font-size: 18px;
                  margin-right: 8px;
                  vertical-align: middle;
                }           
                .date{
                  font-size: 30px;
                  color: $brand-primary;
                  font-weight: 500;
                  font-family: $secondary-font;
                  line-height: 18px;
                  letter-spacing: 0.5px;
                  text-align: center;
                  display: inline-block;
                  vertical-align: middle;
                  /*padding: 12px 0;*/

                  .day_date {
                  }
                  .day_year{
                    display: none;
                  }
                }
              }
              .comment, .author{
                color: $brand-third;
                font-size: 14px;
                font-weight: 400;
                text-align: center;
                display: block;
                margin-bottom: -16px;
                padding: 16px 0;
                opacity: 1;
                font-family: $secondary-font;
                -webkit-transition: all 0.4s;
                -moz-transition: all 0.4s;
                -o-transition: all 0.4s;
                transition: all 0.4s;
                letter-spacing: 0.5px;
                i{
                  font-size: 14px;
                  position: relative;
                  top: -1px;
                }
              }
            }
          }

          .blog_content{
            padding: 30px 10px;
            text-align: center; 
            padding-bottom: 22px;
            margin-bottom: 0;
            .sds_post_title{
              margin-bottom: 8px;
              margin-top: 4px;
              line-height: 18px;
              a{
                font-family: $secondary-font;
                font-size: 16px;
                text-transform: capitalize;
                font-weight: 500;
                line-height: 1.1;
                letter-spacing: 0.5px;
                -webkit-transition: all 0.4s;
                -moz-transition: all 0.4s;
                -o-transition: all 0.4s;
                transition: all 0.4s;
                @media (max-width: 543px) {/* If media is below 544 */
                  font-size: 14px;
                }
                &:hover{
                  color: $brand-secondary;
                }
              }
            }
            .desc{
              margin-bottom: 0;
              font-family: $secondary-font;
              font-size: 14px;
              color:#666;
              font-weight: 400;
              line-height: 18px;
              letter-spacing: 0.5px;
            }
            .article__meta-buttons{
              display: block;
              text-align: center;
              margin: 10px;
              .btn{
                display: inline-block;
                vertical-align: middle;
                font-size: 0px;
                background: none;
                /* margin-right: 3px;*/
                border: 0;
                text-transform: unset;
                padding: 0;
                color: $brand-secondary;
                box-shadow: none;
                -webkit-transition: all 0.5s;
                -moz-transition: all 0.5s;
                -o-transition: all 0.5s;
                transition: all 0.5s;
              }
              i{
                font-size: 20px;
                display: inline-block;
                vertical-align: middle;
                text-align: center;
                color: $brand-secondary;
                position: relative;
                top: 1px;
              }

            }
          }
        }
      }
      .owl-nav{
          position: absolute;
          left: 0;
          right: 0;
          margin: 0 auto;
          top: 41%;
          -webkit-transition: all 0.5s;
          -moz-transition: all 0.5s;
          -o-transition: all 0.5s;
          transition: all 0.5s;
          @media(max-width: 767px){
            position: unset;
            margin-top: 10px;
          }
          .owl-next{
            position: absolute;
            right: -7px;
            @media(max-width: 767px){
              position: unset;
            }
          }
          .owl-prev{
            position: absolute;
            left: -7px;
            @media(max-width: 767px){
              position: unset;
            }
          }
        }
    }
  }
}

.ishi-blog-style2{
  .smartblog{
    .blog-image{
      display: block;
      position: relative;
      width: 100%;
      background-size: cover;
      background-position:center;
      background-repeat: no-repeat;
      &.blog-color {
        display: block;
        position: relative;
        width: 100%;
      }
     }
     .products_block_inner{
      position: relative;
      .sds_blog_post{
        padding: 15px;
          &:hover{
            .blog_post{
              .news_module_image_holder{
                a{
                  img{
                    transform: scale(1.5);
                    -webkit-transform: scale(1.5);
                    -ms-transform: scale(1.5);
                    -o-transform: scale(1.5);
                    -moz-transform: scale(1.5);
                    opacity: 0.4;
                    -webkit-transform-style:unset;
                    transform-style:unset;
                  }
                }
                .hover-date{
                  opacity: 1;
                }
                .blogicons{
                  opacity: 1; 
                }
              }           
            }     
          }
          .blog_post{
            position: relative;
            display: block;
            .news_module_image_holder{
              display: flex;
              align-items:center;
              max-width: 100%;
              position: relative;
              background: #000;
              overflow: hidden;
              
              a{
                position: relative;
                display: block;
                width:100%;
              }
              img{
                -webkit-transition: all 2s ease;
                transition: all 2s ease;
                -moz-transition: all 2s ease;
                -ms-transition:all 2s ease;
                -o-transition: all 2s ease;
                max-width:100%; 
              }
              .hover-date{
                opacity: 0;
                position: absolute;
                left: 0;
                right: 0;
                margin: 0 auto;
                text-align: center;
                color: $light-color; 
                font-family: $secondary-font;
                font-weight: 400;
                -webkit-transition: all .9s ease;
                transition: all .9s ease;
                -moz-transition: all .9s ease;
                -ms-transition:all .9s ease;
                -o-transition: all .9s ease;
                i{
                  display: none;
                }
                .blog_date{
                  border-top: 1px solid rgba(255,255,255,0.4);
                  position: relative;
                  margin-bottom: 0;
                  &:after, &:before{
                    position:absolute;
                    content: "";
                    top: 0;
                    width: 1px;
                    height:86px;
                    background:rgba(255,255,255,0.4); 
                  }
                  &:after{
                    left: 0;
                    right: auto;
                  }
                  &:before{
                    left: auto;
                    right: 0;
                  }
                }
                .date_month{
                  display: inline-block;
                  vertical-align: middle;
                  font-size: 37px;
                  padding: 10px 28px 2px;
                  line-height: 48px;
                }
                .day_year{
                  display:block;
                  font-size: 34px;
                  position: relative;
                  line-height: 44px;
                  &:after, &:before{
                    position:absolute;
                    content: "";
                    top: 22px;
                    width: 60px;
                    height:1px;
                    background:rgba(255,255,255,0.4);
                  }
                  &:after{
                    left: 0;
                    right: auto;
                  }
                  &:before{
                    left: auto;
                    right: 0;
                  }
                }
              }
            }
            .blog_content{
              margin-top:15px;
              margin-bottom:0px;
              transition: all 0.5s;
              -webkit-transition: all 0.5s;
              -moz-transition: all 0.5s;
              -ms-transition: all 0.5s;
              -o-transition: all 0.5s;

              .sds_post_title{
                margin-bottom: 10px;
                line-height: 18px;
                a{
                  font-size: 16px;
                  font-family: $secondary-font;
                  text-transform: capitalize;
                  font-weight: 500;
                  color: $brand-primary;
                  letter-spacing: 0.8px;
                   @media (max-width: 767px) {
                    font-size: 15px;
                   }
                }
              }
              .desc{
                margin-bottom: 0px;
                font-size: 14px;
                letter-spacing: 0.8px;
                color: $brand-success;
                font-weight: 300;
                font-family: $primary-font;
                line-height: 24px;
              }
              .smartbloginfo{
                font-family: $primary-font;
                font-size: 14px;
                font-weight: 400;
                text-transform: capitalize;
                margin-bottom: 10px;
                -webkit-transition: all 0.5s;
                -moz-transition: all 0.5s;
                -o-transition: all 0.5s;
                transition: all 0.5s;
                .article__author,.comment{
                  margin-bottom: 0px;
                  position: relative;
                  margin-right: 10px;
                  color: $brand-secondary;
                  i{
                    display:none;
                    font-size: 16px;
                    color: $brand-secondary;
                    padding-right: 5px;
                  }
                     @media (min-width: 544px) and (max-width: 767px){
                      margin:0 0 5px;
                      padding:0;
                      display:block;
                    }
                     @media (max-width: 349px){
                        margin:0 0 5px;
                        padding:0;
                        display:block;
                     }
                }
                .comment{
                  margin-right: 0;
                }
              }
              .article__meta-buttons{
                display: none;
              }
            }
          }       
      }
    }
   
  }
}


/*--------------------------------------blog page------------------------*/
.smartblog-container{
  .grid__item{
  float:unset;}
  .section-header{
    .article__title{
      font-size: 22px;
      text-transform: uppercase;
      color: $brand-primary;
      font-weight: 500;
      margin-bottom: 20px;
      letter-spacing: 0.8px;
    }
    >span{
      font-size: 15px;
      display: inline-block;
      margin-right: 20px;
      position: relative;
      text-transform: capitalize;
      color: #777777;
      margin-bottom: 30px; 
      padding: 0;
      line-height: normal;
      &:before{
        display:none;
      }
      @media (max-width: 543px) {/* If media is below 544 */
        display: block;
        margin-right: 0;
        margin-bottom: 20px;
      }
      i{
        padding-right: 10px; 
        color: $brand-secondary;
      }
    }
  }
  .article__grid-image-wrapper{
    margin-bottom: 30px; 
    display: block;
    overflow: hidden;
  }
  .rte{
    font-size: 15px;
    text-align: center;
    line-height: 25px;
    letter-spacing: 0.5px;
    color: #333; 
  }
  .share_this_blog{
    text-align: center; 
    a{
      float: none;
    }
  }
  textarea{
    &:focus{
      border-color: #d8dcdc !important; 
    }
  }
  .comment-form {
    .grid{
      >div{
        display: block;
        width: 100%;
      }
      label{
        color: #333;
        font-weight: 500; 
      }
      input{
        height:45px;
      }
    }
  }
  .available-comment{
    display: block;
    width: 100%; 
    .h3{
      font-size: 18px; 
    }
    #comments{
      position: relative;
      li{
        position:relative;
        .comment-img{
          background:url({{'comment-blog.jpg' | asset_url }});
          background-position:0 0 ;
          background-repeat: no-repeat;
          height: 60px;
          width: 60px;
          top: 0;
          left: 0;
          overflow: hidden;
          display: inline-block;
          float: left;
          position: relative;
          margin-right: 20px;
        }
        .comment-inner{
          overflow: hidden;
          position: relative; 
        }
      }
      .comments{
        position: relative;
        display: inline-block;
        .comment__meta{
          >span{
            display: block; 
            &.author{
              font-weight: 600;
              &:before{
                display: none; 
              }
            }
          }
        }
        .comment__content{
          text-align:left; 
        }
      }
    }
  }
}

.smart-form{
  background: $light-color;
  padding: 15px 20px 10px;
}
.smart-user{
  background: $light-color;
  padding: 20px;
}

/*-------------------------------------All-blog-post page------------------------*/
.blog--list-view{
  @media (min-width: 992px) {
    margin:0 -15px;
  }
  .pagination{
     @media (max-width: 991px) {
       padding:0;
     }
  }
}
.sdsarticleCat{
  display: block;
  width: 100%;
  overflow: hidden;
  margin-bottom: 30px;
  text-align: left;
  @media (max-width: 991px) {/* If media is below 992 */
    text-align: center;
  }
  @media (max-width: 543px) {/* If media is below 544 */
    text-align: left;
  }
  .articleContent ,.smartblog-desc{
    @media (max-width: 991px) {/* If media is below 992 */
      padding: 0;
    }
  }
  .smartblog-desc{
    @media (max-width: 991px) {/* If media is below 992 */
      padding: 15px 0px 0;
    }
    position: relative;
    font-family: $primary-font;
    font-size: 14px;
    color: $brand-primary;
    font-weight: 400;
    i{
      margin-right: 5px;
      color: $brand-third;
    }
    .article__author{
      margin: 0px;
      letter-spacing: 1px;
      text-transform: capitalize;
      i{
        color: $brand-secondary;
      }
    }
    .article__title{
      margin: 12px 0;
      line-height: 18px;
      @media screen and (min-width: 992px) and (max-width: 1199px) {/* If media is between 992 and 1199 */
        line-height: 20px;
        margin: 10px 0;
      }
     a{
      font-size: 17px;
      text-transform: uppercase;
      font-weight: 500;
      text-decoration: none;
      font-family: $primary-font;
      letter-spacing: .8px;
      color: $brand-primary;
      @media screen and (min-width: 992px) and (max-width: 1199px) {/* If media is between 992 and 1199 */
        font-size: 17px;
      }
     }
    }
    .blog-details{
      margin-bottom: 10px;
      .article__date{
        margin-bottom: 0;
      }
      span{
        font-size: 15px;
        display: inline-block;
        vertical-align: middle;
        margin-right: 10px;
        position: relative;
        text-transform: capitalize;
        color: #777777;
        line-height: 25px;
        @media screen and (min-width: 992px) and (max-width: 1199px) {/* If media is between 992 and 1199 */
          line-height: 20px;
          font-size: 14px;
        }
      }
    }
    .blog_description{
      margin-bottom: 15px;
      color: #777777;
      line-height: 24px;
      @media screen and (min-width: 992px) and (max-width: 1199px) {/* If media is between 992 and 1199 */
        line-height: 20px;
      }
    }
    .r_more{
      text-decoration: none;
      font-size: 14px;
      font-weight: 500;
      text-transform: unset;
      padding: 8px 15px;
      display: inline-block;
      vertical-align: middle;
      -webkit-transition: all 0.5s;
      -moz-transition: all 0.5s;
      -o-transition: all 0.5s;
      transition: all 0.5s;
    }
  }
}


