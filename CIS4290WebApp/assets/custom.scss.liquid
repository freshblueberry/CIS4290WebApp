      display: block;
      overflow: hidden;
      position: relative;
      text-align: center;
      margin: 0 auto;
      padding: 0;
      @media (max-width: 991px) {/* If media is below 992 */
        padding: 0 !important;
      }
    }
    .service-block{
      display: block;
      overflow: hidden;
      padding: 12px 0;
       @media (max-width: 991px) {/* If media is below 544 */
        padding:  0;
      }


    }
    .services{
        text-align: center;
        cursor: pointer;  
        -webkit-transition: all 0.5s;
        -moz-transition: all 0.5s;
        -o-transition: all 0.5s;
        transition: all 0.5s;
        @media (min-width:544px) and  (max-width: 991px) {/* If media is below 992 */
            text-align:center;
          margin-bottom:20px;
        }
      &:nth-child(3)  , &:nth-child(4){
           @media (min-width:544px) and (max-width: 991px) {/* If media is below 544 */
            margin-bottom:0;
        }
      }
        @media (max-width: 543px) {/* If media is below 544 */
          width: 100%;
          text-align: center;  
          margin-bottom:20px;
        }
      &:last-child{
           @media (max-width:543px) {/* If media is below 544 */
            margin-bottom:0;
        }
      }



        .service-img-outer{
          display: inline-block;
          vertical-align: middle;
          position: relative;
          background: transparent;
          @media screen and (min-width: 992px) and (max-width: 1199px) {/* If media is between 992 and 1199 */
            text-align: center;
            margin: 0 auto;
          }
          @media (max-width: 991px) {/* If media is below 768 */
          display:block;
          }
        }
        .service-img{
          display: inline-block;
          vertical-align: middle;
          position: relative;
          height: 30px;
          width: 30px;
          transition: all 0.7s ease;
          -webkit-transition: all 0.7s ease;
          -moz-transition: all 0.7s ease;
          -ms-transition: all 0.7s ease;
          -o-transition: all 0.7s ease;

          img{
            transition: all 0.7s ease;
            -webkit-transition: all 0.7s ease;
            -moz-transition: all 0.7s ease;
            -ms-transition: all 0.7s ease;
            -o-transition: all 0.7s ease;
          }
          @media(max-width: 991px){
            border:1px solid #eee;
            padding:6px 0;
            height: 45px;
            width: 45px;
            margin-bottom:10px;
          }
        }
        .service-text{
          display: inline-block;
          vertical-align: middle;
          text-align: left;
          padding-left: 10px;
          overflow: hidden;
          position: relative;
          @media screen and (min-width: 992px) and (max-width: 1199px) {/* If media is between 992 and 1199 */
            display: block;
            text-align: center;
            padding: 0;
            width: 100%;
          }
          @media (max-width: 991px) {/* If media is below 768 */
            display: inline-block;
            padding: 0;
            text-align:center;

          }
           @media (max-width: 543px) {/* If media is below 768 */
            text-align:center;
            margin:  0;          
          }
          .service-title{

            font-size: 14px;
            font-weight: 500;
            color:$brand-primary;
            opacity: 1;
            font-family: $primary-font;
            transform: translateY(9px);
            -moz-transform: translateY(9px);
            -webkit-transform: translateY(9px);
            -o-transform: translateY(9px);
            -ms-transform: translateY(9px);
            transition: all 400ms ease;
            -webkit-transition: all 400ms ease;
            -moz-transition: all 400ms ease;
            -ms-transition: all 400ms ease;
            -o-transition: all 400ms ease;
            letter-spacing: 0.5px;
            @media(max-width: 991px){
              transform: translateY(0);
            -moz-transform: translateY(0);
            -webkit-transform: translateY(0);
            -o-transform: translateY(0);
            -ms-transform: translateY(0);
            margin-bottom: 5px;

            }
          }
          .service-desc{
            display: block;
            color:$brand-primary;
            opacity: 0;
            font-weight: 400;
            font-size: 13px;
            transform: translateY(0);
            -moz-transform: translateY(0);
            -webkit-transform: translateY(0);
            -o-transform: translateY(0);
            -ms-transform: translateY(0);
            -webkit-transition: all 400ms ease;
            -moz-transition: all 400ms ease;
            -ms-transition: all 400ms ease;
            -o-transition: all 400ms ease;
            letter-spacing: 0.5px;
            @media screen and (min-width: 992px) and (max-width: 1199px) {/* If media is between 992 and 1199 */
              font-size: 12px;
            }
            @media (max-width: 991px) {/* If media is below 992 */

              opacity: 1;
            }
            @media (max-width: 543px) {/* If media is below 992 */
              font-size: 12px;
              opacity: 1;
            }
          }
        }
        &:hover{     
          .service-text{         
            .service-title{
              opacity: 0;
              transform: translateY(-20px);
              -moz-transform: translateY(-20px);
              -webkit-transform: translateY(-20px);
              -o-transform: translateY(-20px);
              -ms-transform: translateY(-20px);
              @media(max-width: 991px){
                opacity: 1;
                 transform: translateY(0px);
              -moz-transform: translateY(0px);
              -webkit-transform: translateY(0px);
              -o-transform: translateY(0px);
              -ms-transform: translateY(0px);
              }
            }
            .service-desc{
              opacity: 1;
              transform: translateY(-9px);
              -moz-transform: translateY(-9px);
              -webkit-transform: translateY(-9px);
              -o-transform: translateY(-9px);
              -ms-transform: translateY(-9px);
              @media(max-width: 991px){            
                  transform: translateY(0px);
              -moz-transform: translateY(0px);
              -webkit-transform: translateY(0px);
              -o-transform: translateY(0px);
              -ms-transform: translateY(0px);
              }
            }

          }
        }
    }
  }
}

/*================ ishislider ================*/
.ishislider{
  position: relative;
  #ishislideshow-carousel{
    .owl-nav{
      @media (max-width: 767px) {/* If media is below 768 */
        display: none;
      }
      margin-bottom: 0;
      margin-top: 0;
      .owl-prev, .owl-next{
        position: absolute;
        padding:0;
        top: 46%;  
        box-shadow: none;
        margin:4px;
        border-radius: 50%;
        right:auto;
        border:0;
        display: inline-block;
        height: 35px;
        width: 35px;
        background-color: $button-bg-color;
        opacity: 0;
        filter: alpha(opacity=0);
        -webkit-transition: all 0.5s;
        -moz-transition: all 0.5s;
        -o-transition: all 0.5s;
        transition: all 0.5s;

        color: $button-text-color;
        @media (max-width: 991px) {/* If media is below 992 */
          top: 40%;
        }
        &:hover{
          background-color: $button-bg-hover-color;
        }
        i {
          line-height: 35px;
          &:before{
           color: $button-text-color;
           line-height: 35px; 
          }

        }
         &:hover{
            i{
              &:before{
                color:$button-text-hover-color;
              }
              &:after{
                color: $button-text-hover-color;
              }
            }
      }   }
      .owl-next{
        right: 60px;
        left: auto;      
        &:hover{


        }
      }
      .owl-prev{
        left:  60px;
        right:  auto; 
      }
    }
    .owl-dots {
      position: absolute;
      bottom: 5px;
      right: 0;
      left: 0;
      margin: 0 auto;
      text-align: center;
      -webkit-transition: all 0.3s;
      -moz-transition: all 0.3s;
      -o-transition: all 0.3s;
      transition: all 0.3s; 
      .owl-dot {
        display: inline-block;
        vertical-align: middle;
        margin: 0 2px;
        span {
          width: 12px;
          height: 12px;
          background: $brand-primary;
          border-radius: 50%;
          margin: 0 3px;
          display: block;
          -webkit-transition: all 0.3s;
          -moz-transition: all 0.3s;
          -o-transition: all 0.3s;
          transition: all 0.3s; 
        }
        &.active, &:hover {
          span {
            background : $brand-secondary;
          }
        }
      }
    }
    &:hover {
      .owl-nav{
        .owl-prev, .owl-next{
            opacity: 1; 
        }
        .owl-prev{
          left:20px;
        }
        .owl-next{
          right: 20px;
        }
      }
    }
    .slideshow__link{
      display:flex;
      align-items:center;
    }
    .page-width{
      position:absolute;
      left:0;
      right:0;
    }
    .slider-content{
      padding:0;
      .sub-title{
        font-size:30px;
        text-transform: unset;
        line-height: 32px;
        margin-bottom: 20px;
        letter-spacing: 0.5px;
        font-family: $primary-font;
        @media (max-width: 1199px) {
           font-size:25px;
           line-height:26px;
           margin-bottom:15px;
        }
        @media (max-width: 991px) {/* If media is below 992 */
          font-size: 20px;
          line-height: 20px;
        }
         @media (max-width: 543px) {
          font-size: 16px;
          line-height: 18px;
          margin-bottom: 10px;
        }
        @media (max-width: 415px) {
            font-size: 14px;
            line-height: 16px;
            margin-bottom: 8px;
          }
         @media (max-width: 375px) {
           font-size: 13px;
           margin-bottom: 5px;
         }
      }
      .main-title{
        font-size: 86px;
        text-transform: unset;
        line-height: 80px;
        margin-bottom: 15px;
        letter-spacing: 0.5px;
        font-weight: 500;
        font-family: $secondary-font; 
        @media (max-width: 1199px) {
          font-size: 66px;
          line-height:70px;
        }
        @media (max-width: 991px) {
          font-size: 50px;
          line-height: 50px;
        }
        @media (max-width: 767px) {
          font-size: 38px;
          line-height: 40px;
        }
        @media (max-width: 543px) {
          font-size: 28px;
          line-height: 30px;
        }
        @media (max-width: 415px) {
          font-size: 24px;
          line-height: 26px; 
          margin-bottom: 10px;
        }
        @media (max-width: 375px) {
          font-size: 20px;
          line-height: 22px;
          margin-bottom: 7px;
        }
      }
      .desc{
        line-height: 24px;
        margin-bottom: 35px;
        letter-spacing: 0.5px;
        font-weight: 400;
        font-family: $primary-font;
          @media (max-width: 991px) {
             margin-bottom: 30px;
          }
        @media (max-width: 767px) {
          display:none;
        }
        p{
          font-size: 18px;
          letter-spacing: 3px;
          @media (max-width: 991px) {/* If media is below 992 */
          	font-size: 16px;
          }
          @media (max-width: 767px) {
            font-size: 15px;
          }
           @media (max-width: 543px) {
            font-size: 14px;
          }
        }
      }
      .slider-btn{
        @media (max-width: 543px) {
          font-size: 12px;
          padding: 7px 12px;
        }  
         @media (max-width: 415px) {
