$brand-primary: #000000;
$brand-secondary: {{ settings.brand-secondary }};
$brand-third: {{ settings.brand-third }};
$button-bg-color: {{ settings.button-bg-color }};
$button-text-color: {{ settings.button-text-color }};
$button-bg-hover-color: {{ settings.button-bg-hover-color }};
$button-text-hover-color: {{ settings.button-text-hover-color }};
$product-icon-color: {{ settings.product-icon-color }};
$product-icon-hover-color: {{ settings.product-icon-hover-color }};
$navbar-background-color: {{ settings.navbar_background_color }};
$navbar-text-color: {{ settings.navbar-text-color }};
$header-icon-color:{{ settings.header-icon-color }};
$header-icon-hover-color:{{ settings.header-icon-hover-color }};
$menu-bg-color:{{ settings.menu-bg-color }};
$menu-text-color:{{ settings.menu-text-color }};
$menu-text-hover-color:{{ settings.menu-text-hover-color }};
$footer-bg-color: {{ settings.footer-bg-color }};
$footer-heading-color: {{ settings.footer-heading-color }};
$footer-text-color: {{ settings.footer-text-color }};
$footer-text-hover-color: {{ settings.footer-text-hover-color }};
$brand-success: #666666;
$light-color: #ffffff;
$color_text: #333333;
$color-body:#ffffff;
$color-bg: #ffffff;
$primary-font: {{ settings.type_base_family }};
$secondary-font:{% if settings.show_secondary_font %}{{ settings.type_header_family }}{% else %}{{ settings.type_base_family }}{% endif %};

.header-cart-logo{
  display:none;
}
/*================ Header ================*/
#header-layout1{

  /*================ header ================*/
  #header{
    /*------------------------nav-header-----------------*/
    .nav-header{
      padding: 0;
      position: relative;
      background:$navbar-background-color;
      @media (max-width: 767px){
        float: none;
        text-align: center;
        padding: 5px 0;
      }

    }
    .header-block{
      padding: 11px 15px;
      color: $navbar-text-color;
      font-weight: 400;
      font-size: 14px;
      line-height: 19px;
      letter-spacing: 0.5px;
      @media (max-width: 767px) {/* If media is below 768 */
        padding: 5px 0;
      }

    }

    .header-top{
      padding: 25px 0;
      position: relative;
      text-align: center;
      @media (min-width: 992px) {/* If media is above 991 */
        display: flex;
        align-items: center;
      }
      @media (max-width: 991px) {
       overflow:hidden;
      }
    }

    /*---------------------------site-header-inner----------*/
    .header-logo-section{   
      position: relative;
      text-align: left;   

      @media (max-width: 991px) {
        text-align:center;
      }
      .header__logo{
        margin:0;
        a{
          display: block;

          &.site-header__logo-link{
            color: $brand-primary;
            font-weight: 500;
            font-size: 20px;
          }
        }
      }
    }
    .desktop-user-info{
      text-align: right;
      position: relative;
      @media (max-width: 767px) {/* If media is below 768 */
        padding: 5px 0;
        text-align: center;
      }
      .user_info, .checkout, .wishlist,.contact{
        display: inline-block;
        vertical-align: middle;
        padding: 10px 8px;
        position: relative;
        -webkit-transition: all 0.5s;
        -moz-transition: all 0.5s;
        -o-transition: all 0.5s;
        transition: all 0.5s;
        @media(max-width: 767px){
          padding: 0px 8px;
        }
        a{
          color:$navbar-text-color;
          font-weight: 400;
          font-size: 14px;
          line-height: 18px;
          letter-spacing: 0.5px;
          -webkit-transition: all 0.5s;
          -moz-transition: all 0.5s;
          -o-transition: all 0.5s;
          transition: all 0.5s;
        }
      }
      .checkout, .wishlist{
        position: relative;
        &:after{
          content: '';
          position: absolute;
          right: -3px;
          top: 14px;
          background: rgba($navbar-text-color,0.2);
          width: 1px;
          height: 15px;
          @media (max-width: 767px) {
            top: 3px;
          }
        }
      }
      .contact{
        @media (min-width: 768px) {/* If media is above 767 */
          padding-right: 0;
        }
      }
    }
  }

  /*-------------------------search-bar-----------------*/
  #_desktop_search, #_mobile_search{
    cursor: pointer;
    position: relative;
    padding:0 5px;
    display:inline-block;
    @media (max-width: 991px) {
      padding:5px 0;
    }
     &:after{
        position:absolute;
        content:"";
        left:auto;
        right:0;
        width:1px;
        height:16px;
        top:12px;
        background:#a29c9c;
       @media (max-width: 991px){
        display:none;
       }
      }
    
    .search-toggle{
      width: 40px;
      height: 40px;
      display: block;
      -webkit-transition: all 0.5s;
      -moz-transition: all 0.5s;
      -o-transition: all 0.5s;
      transition: all 0.5s; 
      cursor: pointer;
      svg{
        fill:$header-icon-color;
        -webkit-transition: all 0.5s;
        -moz-transition: all 0.5s;
        -o-transition: all 0.5s;
        transition: all 0.5s; 
        @media (max-width: 991px) {
              fill: $button-text-color;
            }
      }
      &:hover{
        @media (min-width: 992px) {
          svg{
            fill:$header-icon-hover-color;
          }
        }
      }
    }
    .search-info{
      position: absolute;
      right: 0;
      top: 50px;
      z-index: 99;
      form{
        width: 100%;
        max-width: 400px;
        .search-tag{
        display:none;}
      }
      .search__input{
        min-width: 300px;
        width: 100%;
        border: 1px solid #e7e7e7;
        border-radius: 0;
        height: 40px;
        padding: 5px 10px;
        margin: 0;
        background:#fff;
        text-transform: capitalize;
        font-size: 13px;
        font-weight: 400;
        text-indent: 10px;
        letter-spacing: 0.8px;
        color:#777;


        @media (max-width: 1199px) {/* If media is below 992 */
          text-indent: 0px;
        }

        @media (max-width: 991px) {
          height:40px;
          min-width: 300px;
        }
        @media (max-width: 543px) {/* If media is below 544 */
          min-width:240px;
        }
        @media (max-width: 375px) {
          min-width:220px;
        }
        &:hover{
          + .btn--link{
            opacity: 1;
          }
        }
        &::-moz-placeholder {
          color: #777;
          opacity:1;
        }
        &::-webkit-input-placeholder {
          color: #777;
          opacity: 1;
        }
        &:focus{
          background: $light-color;
        }
      }
      .search-header__submit{
        position: absolute ;
        right: 0;
        top: 0;
        border: none;
        background:transparent;
        height: 40px;
        width: 40px;
        font-size: 15px;
        font-weight: 400;
        color:$brand-primary;
        line-height: 35px;
        text-align: center;
        padding: 0;
        outline: none;
        z-index: 11;
        -ms-transition: all 0.5s;
        -webkit-transition: all 0.5s;
        -moz-transition: all 0.5s;
        -o-transition: all 0.5s;
        transition: all 0.5s;
        &:place-holder{
          opacity:1;
        }

        @media (max-width: 1199px) {/* If media is below 544 */

        }
        @media (max-width: 991px) {/* If media is below 544 */
          top:0;        
        }
        @media (max-width: 543px) {/* If media is below 544 */

        }


        &.btn--link{
          &:hover, &:focus{
            opacity:1;
          }
        }
        .icon__fallback-text{
          display: none;
          color: $light-color;
          position: unset !important;
          height: 35px;
          width: auto;
          line-height: 32px;
          font-size: 16px;
          -webkit-transition: all 0.5s;
          -moz-transition: all 0.5s;
          -o-transition: all 0.5s;
          transition: all 0.5s;
          @media (max-width: 543px) {/* If media is below 544 */
            font-size: 14px;
          }
        }
        .search-logo{
          display: block;
          i{
         	 color:$header-icon-color;
          }
          &:hover{
            i{
         	 color:$header-icon-hover-color;
          	}
          }
        }
      }
      .search-results{
        li{
          a{
            img{
              border: 1px solid #e9e9e9;
            }
            .price{
              padding-top: 0;
            }
          }
        }
      }
    }
  }
 /*-----------------------User info--------------*/
  #_desktop_user_info, #_mobile_user_info{
      cursor: pointer;
      padding: 0 5px;
      position: relative;
      display:inline-block;
      -webkit-transition: all 0.5s;
      -moz-transition: all 0.5s;
      -o-transition: all 0.5s;
      transition: all 0.5s;
    @media (max-width: 991px) {
      padding:5px 0;
    }
    &:after{
        position:absolute;
        content:"";
        left:auto;
        right:0;
        width:1px;
        height:16px;
        top:12px;
        background:#a29c9c;
      @media (max-width: 991px) {
       display:none;
      }
      }
      &:hover{
        .userinfo-title{
          .userinfo-toggle{
            @media (min-width: 992px) {
              svg{
                fill:$header-icon-hover-color;
              }
            }       
          }
        }
        #userinfo-container{
          top: 80%;
          visibility: visible;
          opacity: 1;
          @media (max-width: 991px){
            top:100%;
          }
        }
      }
      .userinfo-title{
        cursor: pointer;
        .userinfo-toggle{
          width: 40px;
          height: 40px;
          display: inline-block;
          vertical-align: middle;
          svg{
            fill:$header-icon-color;
            display: inline-block;
            vertical-align: middle;
            -webkit-transition: all 0.5s;
            -moz-transition: all 0.5s;
            -o-transition: all 0.5s;
            transition: all 0.5s; 
            @media (max-width: 991px) {
              fill: $button-text-color;
            }
          }
        }
        .user-content{
          display: inline-block;
          vertical-align: middle;
          padding: 5px;
          @media (max-width: 767px) {/* If media is below 767 */
            display: none;
          }
        }  
      }
      .header-title{
        position: relative;
        cursor: pointer;
        .header-heading{  
          font-size: 16px;
          padding-right: 25px;
        }
        .navbar-toggler {
          i {
            position: absolute;
            top: 0;
            right: 0; 
          }
        }
      }  
    }  
  /*-----------------------shopping cart--------------*/
  #_desktop_cart, #_mobile_cart {
    text-align:right;
    white-space: nowrap;
    position: relative;
    padding: 0 5px;
    padding-right:0;
    display: inline-block;
    @media (max-width: 991px) {
      padding:5px 0;
      padding-right: 10px;
    }
    &:hover{
      @media (min-width: 992px) {/* If media is above 991 */
        .cart-display{
          .cart-title{
            .header-cart-logo1{
              svg{
                fill:$header-icon-hover-color;
              }
            }
            .cart-price{
              .main-title{
                color:$brand-secondary;
              }
            }
          }
        } 
      } 
    }
    .cart-display {
      position: relative;
      &:hover{

      }
      .cart-title{
        cursor: pointer;
        .header-cart-logo1{
          width: 40px;
          height: 40px;
          display: inline-block;
          vertical-align: middle;
          -webkit-transition: all 0.5s;
          -moz-transition: all 0.5s;
          -o-transition: all 0.5s;
          transition: all 0.5s;
          @media (max-width: 991px) {/* If media is below 992 */
            height: 40px;
            width: 40px;
            border: 0;
          }
          svg{
            fill:$header-icon-color;
            -webkit-transition: all 0.5s;
            -moz-transition: all 0.5s;
            -o-transition: all 0.5s;
            transition: all 0.5s;
            @media (max-width: 991px) {
              fill: $button-text-color;
            }
          }
        }
        .cart-qty{
            background-color: $brand-primary;
            border-radius: 50%;
            color: $light-color;
            font-size: 11px;
            font-weight: 400;
            height: 15px;
            left: auto;
            position: absolute;
            right: 3px;
            text-align: center;
            top: 2px;
            width: 15px;
            line-height:15px;
            -webkit-transition: all 0.5s;
            -moz-transition: all 0.5s;
            -o-transition: all 0.5s;
            transition: all 0.5s;
            @media (max-width: 991px) {
              background-color: $button-text-color;
              border-radius: 50%;
              color: $button-bg-color;
              top: 4px;
              right: 6px;
              width: 14px;
              line-height:14px;
              height: 14px;
            }
          }
        .cart-price{
          display: none;
          .sub-title{
            text-align: left;
            color: #666666;
            display: block;
            font-size: 13px;
            margin-top: 2px;
            @media(max-width:1199px){
              font-size: 12px;
            }
          }
          .cart-qty{
            @media (max-width: 991px) {/* If media is below 992 */
              background-color: $brand-primary;
              border-radius: 50%;
              color: $light-color;
              font-size: 11px;
              height: 15px;
              left: auto;
              line-height: 14px;
              position: absolute;
              right: 18px;
              text-align: center;
              top: 10px;
              width: 15px;
            }
          }
          .main-title{
            display: block;
            position: relative;
            font-size: 14px;
            color:#000;
            font-weight:  500;
            -webkit-transition: all 0.3s;
            -moz-transition: all 0.3s;
            -o-transition: all 0.3s;
            transition: all 0.3s;
            @media (max-width: 991px) {/* If media is below 992 */
              display: none;
            }
            @media (min-width: 992px) and (max-width:1199px) {/* If media is above 991 */
              font-size: 13px;
            }
          }
        }
      }
      .cart-dropdown-inner {
        position: absolute;
        z-index: 18;
        width: 350px;
        right: 0px;
        top: 50px;
        background: $light-color none repeat scroll 0 0;
        border: 1px solid #ebebeb;
        font-family: $primary-font;
        left: auto;   
        text-align: left;
        line-height: 1.25;
        -webkit-transition: all 0.5s;
        -moz-transition: all 0.5s;
        -o-transition: all 0.5s;
        transition: all 0.5s;
        @media (max-width: 991px) {/* If media is below 992 */
          top: 45px;
          width: 300px;
        }
        @media (max-width: 543px){
          width:280px;
        }
        .product-list{
          max-height: 252px;
          padding: 10px;
          .product{
            padding-bottom: 10px;
            position: relative;
            display: block;
            overflow: hidden;
            &:hover{
              color: $brand-primary;
            }
            &:last-child{
              padding-bottom:0;
            }
            .product-img{
              width: 85px;
              display: inline-block;
              float: left;
              border: 1px solid #eeeeee;
            }
            .product-data{
              overflow: hidden;
              text-align: left;
              padding: 5px 15px;
              span{
                display: block;
                cursor: pointer;
                padding-top: 4px;
              }
              .product-title{
                white-space: normal;
                font-weight: 400;
                color: $brand-primary;
                &:hover{
                  color:$brand-secondary;
                }
              }
            }
            a{
              &.remove{
                position: absolute;
                top: 0;
                right: 0;
                cursor: pointer;
                i{
                  font-size: 20px;
                  color: $brand-primary;
                  &:hover{
                    color:$brand-secondary;
                  }
                }
              }
            }
          }
        }
        .cart__footer {
          padding:15px;
          border-top: 1px solid #eee;
          display: block;
          position: relative;
          overflow: hidden;
          @media (max-width: 543px) {/* If media is below 544 */
            padding: 15px 10px;
          }
          .grid__item{
            width: 100%;
            font-size: 16px;
            float: left;
            overflow: hidden;
            .cart__subtotal{
              float: right;
            }
            .cart-links{
              text-align: center;
              .btn {
                -webkit-transition: all 0.4s;
                -moz-transition: all 0.4s;
                -o-transition: all 0.4s;
                transition: all 0.4s;
                &.view-cart{
                  background-color: $button-bg-hover-color;
                  border-color: $button-bg-hover-color;
                  color: $button-text-hover-color;
                  &:hover , &:focus{
                    background-color: $button-bg-color;
                    border-color:$button-bg-color;
                    color: $button-text-color;
                  }
                }
              }
            }
          }
        }
        .cart__empty{
          padding : 10px;
          color: #999999;
        }
      }
    }
  }
  /*-----------------------header-contact--------------*/
  .header-right{
    text-align: right;
    @media(max-width:991px){
     display:none;
    }
  }
  /*-------------desktop width fixed-header--------------*/
  .navfullwidth, .mobile-width{
    &.fixed-header{ 
      background-color:$button-bg-color;
      position: fixed !important;
      top: 0 !important;
      left: 0;
      width: 100%;
      max-width: 100% !important;
      z-index: 99;
      right: 0;
      padding: 0;
      box-shadow: 0 0 4px 2px rgba(0,0,0,0.1);
      animation: slide-down 0.7s;
      -moz-animation: slide-down 0.7s;
      -webkit-animation: slide-down 0.7s;
      -o-animation: slide-down 0.7s;
      overflow: visible; 
    }
  }

  @keyframes slide-down {
    0% {
      transform: translateY(-100%);
      -webkit-transform: translateY(-100%);
      -ms-transform:translateY(-100%);
      -o-transform:translateY(-100%);
      -moz-transform:translateY(-100%);
    } 
    100% {
      transform: translateY(0);
      -webkit-transform: translateY(0);
      -ms-transform:translateY(0);
      -o-transform:translateY(0);
      -moz-transform:translateY(0);
    } 
  }

  /*--------------------------Navfullwidth------------------*/
#mobile_top_menu_wrapper {
  position: fixed;
  top: 0;
  left: 0;
  width: 300px;
  bottom: 0;
  background: white;
  z-index: 999;
  overflow-y:auto;
  overflow-x: auto;
  #top_menu_closer {
    cursor: pointer;
    padding: 15px 15px 10px;
    text-align: right;
     i{
       font-size:24px;
       color:$brand-primary;
     }
  }
  #_mobile_top_menu {
    padding:0 15px;
  }
  #top-menu {
    margin-bottom: 0;
    text-align:left;
    display: block;
    padding: 0;
  }
  .top-menu {
    .collapse-icons[aria-expanded="true"] {
      .add {
        display: none;
      }
      .remove {
        display: block;
      }
    }
    .collapse-icons .remove {
      display: none;
    }
    .navbar-toggler {
      display: inline-block;
      padding: 0;
      position: absolute;
      right: 0;
      cursor: pointer;
      color: #333;
      i{
        font-size:24px;
      }
    }
    a{
      padding: 0;
        margin:0;
    }
    .collapse {
      display: none;
      &.in {
        display: block;
      }
    }
     
    .sub-menu {
      &.collapse {
        display: none;
        &.in {
          display: block;
        }
      }
      box-shadow: none;
      z-index: inherit;
      display: block;
      position: static;
      overflow: hidden;
      margin-left: 0;
      width: 100%;
      min-width: 100%;
      border: none;
      ul{
        margin: 0;
        padding:0;
        > li {
          float: none;
          margin: 0;
          a {
            text-transform: none;
          }
        }
      }
      ul {
        padding:10px 0 0 15px;
      }
      .mainmenu-dropdown{
        li{
          a{
            display: inline-block;
          }
        }
        .sub-category{
          .top-menu{
            .category{
              padding: 10px 0 5px;
              a{
                 font-size:14px;
              }
            }
          }
        }
      }
    }
    .popover {
      border-radius: 0;
    }
  }
}

.mobile-menu-overlay{
	background-color: rgba(0,0,0,.5);
	height: 100%;
	left: 0;
	opacity: 0;
	position: fixed;
	top: 0;
	transition: opacity 200ms,visibility 0ms 200ms;
	visibility: hidden;
	width: 100%;
	z-index: 998;
  &.active{
    opacity: 1;
    visibility: visible;
  }
}

  /*---------------------------------mobile width--------------*/
  .mobile-width{
    width: 100% !important;
    @media (max-width: 991px) {/* If media is below 992 */
      background-color: $button-bg-color;
      border-top: 0;
    }
    .page-width{
      @media (max-width: 991px) {/* If media is below 992 */
        background-color: $button-bg-color;
      }
    }

    .mobile_search{
      padding: 0 !important; 
    }
    .mobile-width-left{
      float: left;
      padding: 5px;
      #menu-icon{
        cursor: pointer;
        width: 40px;
        height: 40px;
        svg{
          fill:$button-text-color;
        }
      }
    }
    .mobile-width-right{
      float: right;
      >div{
        float: right !important; 
      }
    }
  }

  /*================ main menu ================*/
  .megamenu-header{
    float: none;
  	text-align:center;
    background:$menu-bg-color;
    padding:0;
    width: 50%;
  }

  #_desktop_top_menu {
    padding: 0;
    #top-menu {
      .sub-menu {
        &.collapse {
          opacity: 0;
          box-shadow: 0 0 0 1px rgba(0, 0, 0, 0.1);
        }
        top: 70%;
        left: 15px;
        border: none;
        width: auto;
        z-index: 18;
        overflow:hidden;
        padding: 15px;
        border-radius: 0;
        visibility: hidden;
        text-align: center;
        -webkit-transition: all 0.3s;
        -moz-transition: all 0.3s;
        -o-transition: all 0.3s;
        transition: all 0.3s;
        transform: translateY(60px);
        -webkit-transform: translateY(60px);
        -ms-transform: translateY(60px);
        -o-transform: translateY(60px);
        -moz-transform: translateY(60px);
      }
      > li{
        &:hover {
          &:hover {
             .sub-menu {
              top: 100%;
              opacity: 1;
              transform: translateY(0);
              visibility: visible; 
             }
          }
        }
      }
    }
  }


  #top-menu {
    h3{
      position:relative;   
